   "source": [
    "from PIL import Image, ImageFilter\n",
    "\n",
    "kitten = Image.open('kitten.jpg')\n",
    "blurrykitten = kitten.filter(ImageFilter.GaussianBlur)\n",
    "blurrykitten.save('kitten_blurred.jpg')\n",
    "blurrykitten.show()"
